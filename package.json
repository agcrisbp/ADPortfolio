{
	"private": true,
	"name": "@charisprod/charfolio",
	"version": "3.0.1",
	"description": "Personal Website",
	"author": "agcrisbp <agcrisbp@proton.me>",
	"license": "MIT",
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"analyze": "cross-env ANALYZE=true next build",
		"dev": "next dev --webpack",
		"build": "next build --webpack",
		"start": "next start",
		"postbuild": "next-sitemap",
		"lint": "next lint",
		"type-check": "tsc",
		"format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx,vue}\""
	},
	"dependencies": {
		"@headlessui/react": "2.2.9",
		"@hookstate/core": "4.0.2",
		"@hookstate/localstored": "4.0.2",
		"@iconify/react": "6.0.2",
		"@motionone/dom": "10.18.0",
		"@splitbee/web": "0.3.0",
		"@tailwindcss/line-clamp": "0.4.4",
		"@tailwindcss/typography": "0.5.19",
		"@vercel/analytics": "1.6.1",
		"canvas-confetti": "1.9.4",
		"clsx": "2.1.1",
		"date-fns": "4.1.0",
		"emoji-regex": "10.6.0",
		"inter-ui": "4.1.1",
		"is-web-crawler": "1.1.0",
		"motion": "12.34.0",
		"next": "16.1.6",
		"next-axiom": "1.10.0",
		"next-mdx-remote": "6.0.0",
		"next-themes": "0.4.6",
		"nprogress": "0.2.0",
		"ogl-typescript": "0.1.40",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"react-use": "17.6.0",
		"rehype-autolink-headings": "7.1.0",
		"rehype-raw": "7.0.0",
		"rehype-slug": "6.0.0",
		"remark-code-titles": "0.1.2",
		"remark-emoji": "5.0.2",
		"remark-gfm": "4.0.1",
		"remark-prism": "1.3.6",
		"swr": "2.4.0",
		"tailwindcss": "4.1.18",
		"use-sound": "5.0.0"
	},
	"devDependencies": {
		"@types/canvas-confetti": "1.9.0",
		"@types/node": "25.2.3",
		"@types/nprogress": "0.2.3",
		"@types/react": "19.2.14",
		"@typescript-eslint/eslint-plugin": "8.55.0",
		"@typescript-eslint/parser": "8.55.0",
		"eslint": "9.39.2",
		"eslint-config-next": "16.1.6",
		"gray-matter": "4.0.3",
		"next-sitemap": "4.2.3",
		"prettier": "3.8.1",
		"ts-shader-loader": "2.1.0",
		"typescript": "5.9.3",
		"windicss": "3.5.6",
		"windicss-webpack-plugin": "1.8.0"
	},
	"overrides": {
		"@hookstate/core": {
			"react": "$react"
		}
	}
}
